# LLM API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GOOGLE_API_KEY=...
XAI_API_KEY=xai-...
DEEPSEEK_API_KEY=sk-...
PERPLEXITY_API_KEY=pplx-...

# BigQuery (get from GCP service account)
BQ_SERVICE_ACCOUNT_EMAIL=your-sa@project.iam.gserviceaccount.com
BQ_PRIVATE_KEY=base64-encoded-private-key-pem

# Cloudflare Access (leave empty for local dev - auth is skipped)
# In production, set via wrangler secret put
CF_ACCESS_TEAM_DOMAIN=
CF_ACCESS_AUD=

# Admin API Key (for protecting observation creation/management)
# Generate a random string, e.g.: openssl rand -hex 32
ADMIN_API_KEY=your-admin-api-key-here
